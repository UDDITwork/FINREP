                    <th>Client Name</th>
                    <th>Status</th>
                    <th>Invitation Source</th>
                    <th>Sent At</th>
                    <th>Opened At</th>
                    <th>Completed At</th>
                    <th>Expires At</th>
                    <th>Email Attempts</th>
                    <th>Reminders Sent</th>
                    <th>Notes</th>
                </tr>
            </thead>
            <tbody>
                {{#each clientInvitations}}
                <tr>
                    <td>{{#if this.clientEmail}}{{this.clientEmail}}{{else}}Not Available{{/if}}</td>
                    <td>{{#if this.clientFirstName}}{{this.clientFirstName}}{{else}}Not Available{{/if}} {{#if this.clientLastName}}{{this.clientLastName}}{{else}}{{/if}}</td>
                    <td>
                        <span class="status-badge status-{{this.status}}">{{this.status}}</span>
                    </td>
                    <td>{{#if this.invitationSource}}{{this.invitationSource}}{{else}}Not Available{{/if}}</td>
                    <td>{{#if this.sentAt}}{{formatDate this.sentAt}}{{else}}Not Sent{{/if}}</td>
                    <td>{{#if this.openedAt}}{{formatDate this.openedAt}}{{else}}Not Opened{{/if}}</td>
                    <td>{{#if this.completedAt}}{{formatDate this.completedAt}}{{else}}Not Completed{{/if}}</td>
                    <td>{{#if this.expiresAt}}{{formatDate this.expiresAt}}{{else}}Not Available{{/if}}</td>
                    <td>{{#if this.emailAttempts}}{{this.emailAttempts}}{{else}}0{{/if}}</td>
                    <td>{{#if this.remindersSent}}{{this.remindersSent}}{{else}}0{{/if}}</td>
                    <td>{{#if this.notes}}{{this.notes}}{{else}}No Notes{{/if}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        
        <!-- CAS Upload Data Section -->
        {{#each clientInvitations}}
        {{#if this.casUploadData}}
        <div class="section" style="margin-top: 30px;">
            <h3>CAS Upload Data - {{#if this.clientFirstName}}{{this.clientFirstName}}{{else}}Client{{/if}}</h3>
            <div class="advisor-section">
                <div class="advisor-details">
                    <div><strong>File Name:</strong> <span>{{#if this.casUploadData.fileName}}{{this.casUploadData.fileName}}{{else}}Not Available{{/if}}</span></div>
                    <div><strong>File Path:</strong> <span>{{#if this.casUploadData.filePath}}{{this.casUploadData.filePath}}{{else}}Not Available{{/if}}</span></div>
                    <div><strong>File Size:</strong> <span>{{#if this.casUploadData.fileSize}}{{this.casUploadData.fileSize}} bytes{{else}}Not Available{{/if}}</span></div>
                    <div><strong>Uploaded At:</strong> <span>{{#if this.casUploadData.uploadedAt}}{{formatDate this.casUploadData.uploadedAt}}{{else}}Not Available{{/if}}</span></div>
                    <div><strong>Event ID:</strong> <span>{{#if this.casUploadData.eventId}}{{this.casUploadData.eventId}}{{else}}Not Available{{/if}}</span></div>
                    <div><strong>Password Protected:</strong> <span>{{#if this.casUploadData.password}}Yes{{else}}No{{/if}}</span></div>
                </div>
            </div>
        </div>
        {{/if}}
        {{/each}}
    </div>
    {{/if}}

    <!-- Meeting History -->
    {{#if meetings.length}}
    <div class="section">
        <h2>Meeting History</h2>
        <table>
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Type</th>
                    <th>Duration</th>
                    <th>Key Points</th>
                </tr>
            </thead>
            <tbody>
                {{#each meetings}}
                <tr>
                    <td>{{date}}</td>
                    <td>{{type}}</td>
                    <td>{{duration}}</td>
                    <td>{{keyPoints}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    {{/if}}

    <!-- Footer -->
    <div class="footer">
        <p><strong>{{vault.firmName}}</strong> | {{vault.advisorName}} | SEBI Registration: {{vault.sebiRegNumber}}</p>
        <p>This report is confidential and intended solely for the use of {{client.name}}.</p>
        <p>Generated on {{reportMetadata.generatedAt}} | Report Version {{reportMetadata.reportVersion}}</p>
    </div>
</body>
</html>
